
@{
	ViewBag.Title = "Search By Location";
}
<h2>@ViewBag.Title</h2>

<style>
	.user-image-container img {
		height: 100px;
		position: absolute;
	}
</style>

@if (true) {
	@Html.Partial("UserTabMenu")

	<div>
		<h4>Search the Tri-state for your Favorite Bakeries!</h4>
		<iframe width="900" height="700" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"
				src="https://cstatebakers.maps.arcgis.com/apps/webappviewer/index.html?id=29fe9a50bad04e0086dd0d399d1a5c2d" allow='geolocation'>
		</iframe>
	</div>
}

<script>
	$(document).ready(function () {
		SetActiveMenus('profile', 'gallery');
	});
</script>